<table class="table table-bordered"> 
      <tr>
        <th>User</th>
        <th>Sport</th>
        <th>Date</th>
        <th>Start At</th>
        <th>End At</th>
        <th>Address</th>
        
        <!-- <th>Rating</th> -->
       
      </tr>
    
    <% @mymeetings.each do |mymeeting| %>
        <tr>
          <td><%= current_user.first_name %></td>
          <td><%= mymeeting.sport.name unless nil? %></td>
          <td><%= mymeeting.meetdate.try(:strftime, '%A  %B  %e')  %></td>
          <td><%= mymeeting.start_at.strftime('%I : %M %P') %></td>
          <td><%= mymeeting.end_at.strftime('%I : %M %P') %></td>
         <!--  <td><%#= distance_of_time_in_words mymeeting.start_at, mymeeting.end_at %></td> -->
          <td><%= mymeeting.address %></td>
        </tr>

        <% @meetings.each do |meeting| %>
            <%#if mymeeting.overlaps?(meeting) %>
              <tr>
                <td><%= meeting.user.first_name %></td>
                <td><%= meeting.meeting.sport.name %></td>
                <td><%= meeting.meeting.meetdate.try(:strftime, '%A  %B  %e')  %></td>
                <td><%= meeting.meeting.start_at.strftime('%I : %M %P') %></td>
                <td><%= meeting.meeting.end_at.strftime('%I : %M %P') %></td>
                <td><%= meeting.meeting.address %></td>
                <!-- <td><%#= meeting.user.sport.user_sport.rating %></td> -->
              </tr>
              <%#end%>
            <%end%>
        
    <%end%>
</table>