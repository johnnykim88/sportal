<div  class="contents"> 
    <div class="container">	
		<div  class="row">

    <table class="table table-bordered"> 
      <tr>
        <th>User</th>
        <th>Sport</th>
        <th>Date</th>
        <th>Start Time</th>
        <th>Time Frame</th>
        <th>Location</th>
        <th>Invite</th>
        
      </tr>
     	<% unless @matchmeetings.nil? %>
     	<% @matchmeetings.each do |meeting|  %>
        	<%unless meeting.user_meetings.first.user == current_user %>
        <tr>
          
          <td><%= link_to meeting.user_meetings.first.user.first_name, profile_path(meeting.user_meetings.first.user.id)  %></td>
          <td><%= meeting.sport.name unless nil? %></td>
          <td><%= meeting.meetdate.try(:strftime, '%A  %B  %e')  %></td>
          <td><%= meeting.start_at.strftime('%I : %M %P') %></td>
          <td><%= distance_of_time_in_words meeting.start_at, meeting.end_at %></td>
          <td><%= meeting.address %></td>
          <td></td>
          
        </tr>
     <%end%>
  
    <%end%>
    <%end%>
    </table>
</div>



		  	</div>
		</div>
